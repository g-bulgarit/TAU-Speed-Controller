{
  "name": "Unicko Speed Controller",
  "version": "0.1.2",
  "description": "Make video lectures fast again",
  "manifest_version": 2,

  "browser_action" : {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["http://*.unicko.com/*","https://*.unicko.com/*"],
      "css": ["popup.css"],
      "js": ["popup.js"]
    }
  ],

"permissions": ["http://*.unicko.com/*", "https://*.unicko.com/*"]
}