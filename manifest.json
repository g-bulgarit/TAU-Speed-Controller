{
  "name": "Unicko Speed Controller",
  "version": "1.0",
  "description": "Make video lectures fast again",
  "manifest_version": 2,

  "browser_action" : {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["popup.js"]
    }
  ],

"permissions" : [
  "activeTab"
  ]
}